  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "lint": "eslint src test",
    "test": "pta --no-esm test/*.js",
    "test-watch": "onchange test/*.js -- pta --no-esm test/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "eslint --cache --fix"
    ],
    "*.md": "prettier --write"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-es5": "^1.5.0",
    "eslint-plugin-prettier": "^3.1.4",
    "globrex": "^0.1.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "matcher": "^3.0.0",
    "onchange": "^7.0.2",
    "picomatch": "^2.2.2",
    "prettier": "^2.0.5",
    "pta": "^0.1.3"
  }
}
